# Runtime
NODE_ENV=development
PORT=8080
LOG_LEVEL=info

# Postgres
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/educhain

# Sui network
SUI_NETWORK=testnet
# Optional override (otherwise derived from SUI_NETWORK):
SUI_RPC_URL=

# Published Move package
SUI_PACKAGE_ID=0x...

# Shared objects (fill after init_state)
SUI_COURSE_CATALOG_ID=0x...
SUI_PROPOSAL_REGISTRY_ID=0x...

# Capability objects (fill after init_state)
SUI_TEACHER_CAP_ID=0x...
SUI_ADMIN_CAP_ID=0x...
SUI_ISSUER_CAP_ID=0x...

# Indexer settings
INDEXER_POLL_INTERVAL_MS=2500
INDEXER_EVENT_FILTER_MODE=package
INDEXER_MODULE_NAME=educhain
INDEXER_EVENT_TYPE=

# DEMO ONLY: server-side signing
# base64 (recommended) or 0xHEX secret key bytes.
ADMIN_PRIVATE_KEY=
